
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Role{
  ADMIN
  USER
}

model User{
  id String @default(uuid()) @db.Uuid()
  name String
  email String @unique
  password String
  image String?
  role Role? 

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
} 